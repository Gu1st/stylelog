{
  "name": "@harexs/style-log",
  "version": "1.0.0",
  "type": "module",
  "description": "a simple console style log for devtools or nodejs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "source": "src/index.ts",
  "author": {
    "name": "Harexs",
    "email": "con1y@qq.com"
  },
  "license": "MIT",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev":"esno src/index.ts",
    "build": "unbuild",
    "login": "npm login --registry https://registry.npmjs.org/",
    "pub": "npm publish --access public --registry https://registry.npmjs.org/",
    "commit": "git add . && git-cz",
    "release": "release-it"
  },
  "devDependencies": {
    "esno": "^0.17.0",
    "git-cz": "^4.9.0",
    "release-it": "^16.1.3",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  }
}
